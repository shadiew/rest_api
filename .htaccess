# enable mod_rewrite
RewriteEngine On

# RewriteCond = define rule condition
# HTTP_REFERER = check from where the request originated
# ! = exclude
# ^ = start of string 
# [NC] = case insensitive search 
RewriteCond %{HTTP_REFERER} !^https://preeminent-bubblegum-5cf0ce.netlify.app [NC]

# \ = match any
# . = any character
# () = pattern, group 
# $ = end of string 

# [F] = forbidden, 403
# [L] = stop proccessing further rules 
RewriteRule \.(php|css|js)$ - [F,L]